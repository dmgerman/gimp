#: app/main.c:285
msgid "Valid options are:\n"
msgstr "Допустимые параметры:\n"

#: app/main.c:286
msgid "  -h --help                Output this help.\n"
msgstr "  -h --help                Вывести эту подсказку.\n"

#: app/main.c:287
msgid "  -v --version             Output version info.\n"
msgstr "  -v --version             Вывести информацию о версии.\n"

#: app/main.c:288
msgid "  -b --batch <commands>    Run in batch mode.\n"
msgstr "  -b --batch <Befehle>     Запустить в командном режиме.\n"

#: app/main.c:289
msgid "  -g --gimprc <gimprc>     Use an alternate gimprc file.\n"
msgstr "  -g --gimprc <gimprc>     Использовать альтернативный файл gimprc.\n"

#: app/main.c:290
msgid "  -n --no-interface        Run without a user interface.\n"
msgstr "  -n --no-interface        Запустить без интерфейса пользователя.\n"

#: app/main.c:291
msgid "  -r --restore-session     Try to restore saved session.\n"
msgstr ""
"  -r --restore-session     Попытаться восстановить сохраненный сеанс.\n"

#: app/main.c:292
msgid ""
"  --no-data                Do not load patterns, gradients, palettes, "
"brushes.\n"
msgstr ""
" --no-data                Не загружать шаблоны, градиенты, палитры и кисти.\n"

#: app/main.c:293
msgid "  --verbose                Show startup messages.\n"
msgstr "  --verbose                Показывать сообщения при старте.\n"

#: app/main.c:294
msgid "  --no-splash              Do not show the startup window.\n"
msgstr "  --no-splash              Не показывать окно запуска.\n"

#: app/main.c:295
msgid "  --no-splash-image        Do not add an image to the startup window.\n"
msgstr "  --no-splash-image        Не добавлять изображение к окну запуска.\n"

#: app/main.c:296
msgid ""
"  --no-shm                 Do not use shared memory between GIMP and its "
"plugins.\n"
msgstr ""
"  --no-shm                 Не использовать разделяемую память между GIMP и "
"его дополнениями.\n"

#: app/main.c:297
msgid "  --no-xshm                Do not use the X Shared Memory extension.\n"
msgstr ""
"  --no-xshm                Не использовать расширение X Shared Memory.\n"

#: app/main.c:298
msgid ""
"  --console-messages       Display warnings to console instead of a dialog "
"box.\n"
msgstr ""
"  --console-messages       Показывать предупреждения на консоли вместо "
"диалоговых окон.\n"

#: app/main.c:299
msgid "  --debug-handlers         Enable debugging signal handlers.\n"
msgstr "  --debug-handlers         Разрешить отладку обработчиков сигналов.\n"

#: app/main.c:300
msgid ""
"  --display <display>      Use the designated X display.\n"
"\n"
msgstr ""
"  --display <дисплей>      Использовать указанный X-дисплей.\n"
"\n"

#: app/main.c:301
msgid "  --system-gimprc <gimprc> Use an alternate system gimprc file.\n"
msgstr ""
"  --system-gimprc <gimprc> Использовать альтернативный системный файл "
"gimprc.\n"

