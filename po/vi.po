msgid "  --verbose                Show startup messages.\n"
msgstr "  --verbose                Hiển thị thông điệp khởi chạy.\n"

#: app/main.c:499
msgid ""
"  --no-shm                 Do not use shared memory between GIMP and "
"plugins.\n"
msgstr ""
"  --no-shm                 Không dùng bộ nhớ chia sẻ giữa GIMP và plugins.\n"

