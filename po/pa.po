#: app/main.c:490
msgid "GIMP version"
msgstr "ਜੈਮਪ ਵਰਜਨ"

#: app/main.c:498
#, c-format
msgid ""
"\n"
"Usage: %s [option ... ] [file ... ]\n"
"\n"
