#: app/main.c:501
msgid "GIMP version"
msgstr "GIMP versija"

#: app/main.c:509
#, c-format
msgid ""
"\n"
"Usage: %s [option ... ] [file ... ]\n"
"\n"
msgstr ""
"\n"
